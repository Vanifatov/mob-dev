<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.mirea.vanifatov.mireaproject.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.mirea.vanifatov.mireaproject.ui.gallery.GalleryFragment"
        android:label="@string/menu_gallery"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.mirea.vanifatov.mireaproject.ui.slideshow.SlideshowFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/fragment_slideshow" />

    <fragment
        android:id="@+id/nav_Data"
        android:name="com.mirea.vanifatov.mireaproject.DataFragment"
        android:label="fragment_data"
        tools:layout="@layout/fragment_data" />

    <fragment
        android:id="@+id/nav_WebView"
        android:name="com.mirea.vanifatov.mireaproject.WebViewFragment"
        android:label="fragment_web_view"
        tools:layout="@layout/fragment_web_view" />

    <fragment
        android:id="@+id/nav_Worker"
        android:name="com.mirea.vanifatov.mireaproject.WorkerFragment"
        android:label="fragment_worker"
        tools:layout="@layout/fragment_worker" />

    <fragment
        android:id="@+id/nav_geo"
        android:name="com.mirea.vanifatov.mireaproject.GeoFragment"
        android:label="Geo Fragment"
        tools:layout="@layout/fragment_geo" />

    <fragment
        android:id="@+id/nav_profile"
        android:name="com.mirea.vanifatov.mireaproject.ProfileFragment"
        android:label="Profile Fragment"
        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/nav_voicemsg"
        android:name="com.mirea.vanifatov.mireaproject.VoiceMesgFragment"
        android:label="Voice Message Fragment"
        tools:layout="@layout/fragment_voice_mesg" />

    <fragment
        android:id="@+id/nav_user_profile"
        android:name="com.mirea.vanifatov.mireaproject.UserProfileFragment"
        android:label="User profile"
        tools:layout="@layout/fragment_user_profile" />

    <fragment
        android:id="@+id/nav_convert_files"
        android:name="com.mirea.vanifatov.mireaproject.ConvertFilesFragment"
        android:label="Convert Files"
        tools:layout="@layout/fragment_convert_files" />

    <fragment
        android:id="@+id/nav_web_firebase"
        android:name="com.mirea.vanifatov.mireaproject.FragmentFireBase"
        android:label="Web Firebase"
        tools:layout="@layout/fragment_fire_base">

        <action
            android:id="@+id/navigation_fb_web"
            app:destination="@id/nav_web_fb" />

    </fragment>

    <fragment
        android:id="@+id/nav_web_fb"
        android:name="com.mirea.vanifatov.mireaproject.FragmentWeb"
        android:label="Fragment web"
        tools:layout="@layout/fragment_web" />

</navigation>